{
    "env": {
        "node": true,
        "es6": true,
        "jest": true
    },
    "plugins": ["jest", "prettier"],
    "extends": ["google", "plugin:prettier/recommended"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "rules": {
        "prettier/prettier": "error",
        "require-jsdoc": 0
    }
}
